{
  "name": "auto-save",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "author": "jonathan",
  "description": "Database automation tool",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--experimental-modules concurrently \"vite\" \"electron .\" \" npm run run:server\"",
    "run:server": "npx kill-port 5000 && nodemon --watch src/api --ext ts --exec tsx src/api/server.ts",
    "build": "vite build && electron-builder",
    "start": "electron-forge start --config=forge.config.cjs",
    "lint": "eslint .",
    "preview": "vite preview",
    "package": "electron-forge package --config=forge.config.cjs",
    "make": "electron-forge make --config=forge.config.cjs"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tailwindcss/postcss": "^4.0.4",
    "@tailwindcss/vite": "^4.0.4",
    "@tanstack/react-table": "^8.20.6",
    "archiver": "^7.0.1",
    "check-disk-space": "^3.4.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "diskusage": "^1.2.0",
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.21.2",
    "lucide-react": "^0.474.0",
    "msnodesqlv8": "^4.5.0",
    "mssql": "^11.0.1",
    "next-themes": "^0.4.4",
    "nodemailer": "^6.10.0",
    "react": "^19.0.0",
    "react-bootstrap": "^2.10.9",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.5",
    "react-router-dom": "^7.1.5",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.3"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.6.1",
    "@electron-forge/maker-deb": "^7.6.1",
    "@electron-forge/maker-rpm": "^7.6.1",
    "@electron-forge/maker-squirrel": "^7.6.1",
    "@electron-forge/maker-zip": "^7.6.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.6.1",
    "@electron-forge/plugin-fuses": "^7.6.1",
    "@electron/fuses": "^1.8.0",
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.13.7",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/tailwindcss": "^3.1.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^34.1.1",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.1",
    "tailwindcss": "^4.0.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  },
  "build": {
    "appId": "com.example.app",
    "productName": "autoSave",
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "icon": "assets/icon.icns"
    },
    "win": {
      "icon": "assets/icon.ico"
    },
    "linux": {
      "icon": "assets/icon.png"
    }
  }
}
